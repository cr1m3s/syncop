<RIBCL VERSION="2.0">
  <LOGIN USER_LOGIN="Administrator" PASSWORD="{PSWD}">
    <RIB_INFO MODE="write">
      <MOD_NETWORK_SETTINGS>
        <DHCP_ENABLE VALUE="N"/>
        <DNS_NAME VALUE="{HOST}"/>
        <IP_ADDRESS VALUE="{IP}"/>
        <SUBNET_MASK VALUE="{MASK}"/>
        <GATEWAY_IP_ADDRESS VALUE="{GATEWAY}"/>
      </MOD_NETWORK_SETTINGS>
    </RIB_INFO>
    <USER_INFO MODE="write">
      <ADD_USER USER_NAME="admin" USER_LOGIN="admin" PASSWORD="AWESOME1">
        <ADMIN_PRIV value="Y"/>
        <REMOTE_CONS_PRIV value="Y"/>
        <RESET_SERVER_PRIV value="Y"/>
        <VIRTUAL_MEDIA_PRIV value="Y"/>
        <CONFIG_ILO_PRIV value="Y"/>
      </ADD_USER>
    </USER_INFO>
  </LOGIN>
</RIBCL>
