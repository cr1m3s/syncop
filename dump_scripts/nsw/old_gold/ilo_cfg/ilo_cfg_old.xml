<RIBCL VERSION="2.0">
   <LOGIN USER_LOGIN="Administrator" PASSWORD="{PSWD}">
    <RIB_INFO MODE="write">
      <MOD_NETWORK_SETTINGS>           
        <DHCP_ENABLE VALUE="N"/>
        <DHCP_GATEWAY VALUE="N"/>
        <DHCP_DNS_SERVER VALUE="N"/>
        <DHCP_WINS_SERVER VALUE="N"/>
        <DHCP_STATIC_ROUTE VALUE="N"/>
        <DHCP_DOMAIN_NAME VALUE="N"/>
        <DHCP_SNTP_SETTINGS VALUE="N"/>
	<HOST_NAME VALUE="{HOST}"/>
        <IP_ADDRESS VALUE="{IP}"/>
        <SUBNET_MASK VALUE="{MASK}"/>
	<GATEWAY_IP_ADDRESS VALUE="{GATEWAY}"/>
    	</MOD_NETWORK_SETTINGS>
    </RIB_INFO>
  </LOGIN>
</RIBCL>
